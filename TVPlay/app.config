<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="Plugins"/>
    </assemblyBinding>
  </runtime>
  <connectionStrings>
    <add name="tasConnectionString" connectionString="SERVER=localhost;DATABASE=tas;UID=root;PASSWORD=;Keepalive=300;charset=utf8;" providerName="MySql.Data.MySqlClient"/>
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <appSettings>
    <add key="IngestFolders" value="Configuration\IngestFolders.xml"/>
    <add key="LocalDevices" value="Configuration\LocalDevices.xml"/>
    <add key="TempDirectory" value="C:\TEMP\media\"/>
	<add key="DatabaseType" value="SQLite" />
	<add key="Instance" value="0"/>
	<add key="ReferenceLoudnessLevel" value="-23" />
	<add key="IsBackupInstance" value="False" />
  </appSettings>
</configuration>
