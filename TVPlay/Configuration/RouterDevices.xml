    </OutputPorts>
  </RouterDevice>-->
  
    <RouterDevice EngineName="TVP Szczecin" Type="Nevion" IpAddress="192.168.0.1" Port="4381" Level="1" Login="admin" Password="password" SwitchOnLoad="true" SwitchDelay="0">
      <OutputPorts>
